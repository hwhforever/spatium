{
  "buildCommand": "cd spatium-v2 && npm install && npm run build",
  "outputDirectory": "spatium-v2/dist",
  "devCommand": "cd spatium-v2 && npm run dev",
  "installCommand": "cd spatium-v2 && npm install",
  "functions": {
    "spatium-v2/api/generate.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/spatium-v2/api/$1"
    }
  ]
}
